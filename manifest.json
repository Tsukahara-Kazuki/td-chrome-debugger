{
  "manifest_version": 3,
  "name": "TD Debugger",
  "version": "0.1.1",
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "webRequest"
  ],
  "host_permissions": [
    "*://*/*/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ]
}
